.header-wrapper
  position relative
  width 99%
  height 84px
  top 8px
  margin-left 0.5%
  border 1px solid rgb(28, 125, 160)
  border-radius 5px
  box-shadow 1px 1px 10px 0 rgb(28, 125, 160)
  text-align center
  .logo
    position absolute
    top 1px
    left 3px
    width 72px
    height 72px
  .logo-name
    position absolute
    left 68px
    font-size 24px
    font-weight 500
    line-height 55px
  .page-name
    font-size 32px
    font-weight 500
    line-height 84px
